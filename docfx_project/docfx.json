{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "Albatross.CodeAnalysis/Albatross.CodeAnalysis.csproj",
            "Albatross.CodeAnalysis.Polyfill/Albatross.CodeAnalysis.Polyfill.csproj",
            "Albatross.CodeAnalysis.Testing/Albatross.CodeAnalysis.Testing.csproj"
          ]
        }
      ],
      "output": "api"
    }
  ],
  "build": {
    "sitemap": {
      "baseUrl": "https://rushuiguan.github.io/codeanalysis/",
      "priority": 0.5,
      "changefreq": "weekly"
    },
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "*.html",
          "robots.txt"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Albatross.CodeAnalysis",
      "_appTitle": "Albatross.CodeAnalysis",
      "_appLogoPath": "./images/logo-transparent.svg",
      "_appFaviconPath": "./images/favicon.svg",
      "_enableSearch": true,
      "pdf": false,
      "_gitContribute": {
        "repo": "https://github.com/rushuiguan/codeanalysis",
        "branch": "main",
        "path": "docfx_project"
      },
      "_gitUrlPattern": "github"
    }
  }
}